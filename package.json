{
  "name": "babelon-protocol",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prebuild": "node scripts/increment-version.js",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "echo 'No tests configured yet' && exit 0",
    "test:coverage": "echo 'Coverage testing not configured yet' && exit 0",
    "preview": "vite preview",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "audit": "npm audit --audit-level moderate",
    "audit:fix": "npm audit fix",
    "outdated": "npm outdated",
    "update": "npm update",
    "clean": "rm -rf dist node_modules/.vite",
    "increment-version": "node scripts/increment-version.js",
    "prebuild": "npm run increment-version && npm run clean",
    "postbuild": "echo 'Build completed successfully'"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.4.0",
    "@openzeppelin/contracts-upgradeable": "^5.0.0",
    "@tanstack/react-query": "^5.90.2",
    "ethers": "^6.15.0",
    "lucide": "^0.544.0",
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.12.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@nomicfoundation/hardhat-ethers": "^3.1.3",
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "@nomicfoundation/hardhat-verify": "^2.1.3",
    "@openzeppelin/hardhat-upgrades": "^3.9.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "dotenv": "^17.2.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "hardhat": "^2.28.3",
    "terser": "^5.44.0",
    "vite": "^7.1.7"
  }
}
